<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IProductService" />
                <binding name="BasicHttpBinding_IProductLineService" />
                <binding name="BasicHttpBinding_ICustomerOrderService" />
                <binding name="BasicHttpBinding_ICustomerService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8080/Service" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IProductService" contract="ProductServiceReference.IProductService"
                name="BasicHttpBinding_IProductService" />
            <endpoint address="http://localhost:8081/Service" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IProductLineService"
                contract="ProductLineServiceReferencee.IProductLineService"
                name="BasicHttpBinding_IProductLineService" />
            <endpoint address="http://localhost:8082/Service" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_ICustomerOrderService"
                contract="CustomerOrderServiceReference.ICustomerOrderService"
                name="BasicHttpBinding_ICustomerOrderService" />
            <endpoint address="http://localhost:8083/Service" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_ICustomerService" contract="CustomerServiceReference.ICustomerService"
                name="BasicHttpBinding_ICustomerService" />
        </client>
    </system.serviceModel>
</configuration>